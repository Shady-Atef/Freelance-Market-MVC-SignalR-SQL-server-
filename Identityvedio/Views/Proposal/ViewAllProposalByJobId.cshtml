
@using Identityvedio.Models

@model List<Proposal>
@{
    ViewBag.Title = "ViewAllProposalByJobId";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ViewAllProposalByJobId</h2>

<table>
    <tr style="padding:10px;">
        <td style="padding:10px;">Proposal Text</td>
        <td style="padding:10px;">FreeLancer Name</td>
        <td style="padding:10px;">Proposal Date</td>
        <td style="padding:10px;">  </td>
    </tr>
    @foreach (var item in Model)
    {
        <tr style="padding:10px;">
            <td style="padding:10px;">@item.proposal</td>
            <td style="padding:10px;">@item.ApplicationUser.UserName</td>
            <td style="padding:10px;">@item.proposaldate</td>

            <td style="padding:10px;"><a href="\Job\Index">BacK</a></td>
        </tr>

    }
</table>



